    <Button
      title="Change to Videos"
      onPress={() => this.props.navigation.navigate('Video')}
      />
      <h3></h3>

    <Button
      title="Change to MP3"
      onPress={() => this.props.navigation.navigate('MP3')}
      />
      <h1></h1>

    Video: {
        screen: VideoViewer
        },
        MP3: {
        screen: AudioListener
        }